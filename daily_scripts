h_ = (TH1D*) ttree_out->GetHistogram()

h_nom = (TH1D*) h_->Clone()
h_nom->SetDirectory(0)
h_nom->SetName("nom")

h_up = (TH1D*) h_->Clone()
h_up->SetDirectory(0)
h_up->SetName("up")

h_down = (TH1D*) h_->Clone()
h_down->SetDirectory(0)
h_down->SetName("down")

h_up->SetLineColor(kRed)
h_down->SetLineColor(kGreen)

h_nom->Draw()
h_up->Draw("same")
h_down->Draw("same")

h_up->Divide(h_nom)
h_down->Divide(h_nom)
h_up->Draw("")
h_down->Draw("same")

